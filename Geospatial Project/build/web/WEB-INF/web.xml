<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.0" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd">
    <property name="libs.CopyLibs.classpath" value="bin/copylibstask.jar"/>
    <welcome-file-list>
    <welcome-file>index.html</welcome-file>
    </welcome-file-list>
    <servlet>
        <servlet-name>MoranRCtrl</servlet-name>
        <servlet-class>ApplicationControllers.MoranRCtrl</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>FileAccessCtrl</servlet-name>
        <servlet-class>ApplicationControllers.FileAccessCtrl</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>MoranRCtrl</servlet-name>
        <url-pattern>/MoranRCtrl</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>FileAccessCtrl</servlet-name>
        <url-pattern>/FileAccessCtrl</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
